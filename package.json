{
  "name": "nest-sls-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:sq-module": "NODE_LOCAL_ENV=dev &&  nx build  &&  nx serve sq-module --verbose",
    "prebuild:sq-module": "rimraf dist/apps/sq-module",
    "build:docker:nx:sq-module:local": " ./apps/sq-module/build/build.sh dev",
    "build:docker:nx:sq-module:test": " ./apps/sq-module/build/build.sh test",
    "build:docker:nx:sq-module:prod": " ./apps/sq-module/build/build.sh prod",
    "build:sq-module:local": "npm run prebuild:sq-module && NODE_LOCAL_ENV=dev && npm run build:nx:sq-module:local",
    "build:webpack:sq-module:local": "npm run prebuild:sq-module && NODE_LOCAL_ENV=dev && webpack --config ./apps/sq-module/webpack-serve.config.js",
    "build:nx:sq-module:local": "NODE_LOCAL_ENV=dev && nx build sq-module --verbose",
    "build:nx:sq-module": "nx build sq-module --verbose",
    "test": "nx test",
    "format": "nx format:write --all",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
